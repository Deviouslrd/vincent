{
  "plugins": ["solid"],
  "extends": [
    "eslint:recommended",
    "plugin:solid/typescript",
    "@electron-toolkit/eslint-config-ts/recommended"
  ],
  "rules": {
    "indent": [
      "warn",
      2,
      {
        "SwitchCase": 1,
        "ignoreComments": true
      }
    ],
    "semi": [
      "warn",
      "always"
    ],
    "quotes": [
      "warn",
      "backtick"
    ]
  }
}
